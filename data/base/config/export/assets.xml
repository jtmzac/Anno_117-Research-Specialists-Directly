                          <UnlockNeeded>37721</UnlockNeeded>
					</ConditionUnlocked>
					<ConditionPropsNegatable />
				</Values>
			</TriggerCondition>
			<TriggerActions>
				<Item>
					<TriggerAction>
						<Template>ActionUnlockAsset</Template>
						<Values>
							<Action />
							<ActionUnlockAsset>
								<UnhideAssets>
									<Item>
										<Asset>1400300226</Asset>
									</Item>
									</UnhideAssets>
								<UnlockAssets>
									<Item>
										<Asset>1400300226</Asset>
									</Item>
								</UnlockAssets>
							</ActionUnlockAsset>
						</Values>
					</TriggerAction>
				</Item>
			</TriggerActions>
        </Trigger>
        <TriggerSetup>
          <AutoRegisterTrigger>1</AutoRegisterTrigger>
          <UsedByFirstParties>1</UsedByFirstParties>
        </TriggerSetup>
      </Values>
    </Asset>








